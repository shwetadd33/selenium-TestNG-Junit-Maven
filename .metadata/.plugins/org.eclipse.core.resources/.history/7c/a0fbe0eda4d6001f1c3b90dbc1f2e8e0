package testcases;

import java.util.ArrayList;
import java.util.List;

import org.testng.annotations.DataProvider;
import org.testng.annotations.Test;

public class ParameterizesTestcases {
	
	@Test(dataProvider = "getData")
	public void create(String userName, String userPass, int phone) {
		
		System.out.println("User is created" + "---" + userName + "---" + userPass + "---" + phone);
	}

	@Test
	public void delete() {
		
	}
	
	
	@DataProvider
	public Object[][] getData(){
		Object[][] data = new Object[3][3];
		
		data[0][0] = "ABC";
		data[0][1] = "Pass1";
		data[0][2] = 123;
		
		data[1][0] = "PQR";
		data[1][1] = "Pass2";
		data[1][2] = 123;
		
		data[2][0] = "XYZ";
		data[2][1] = "Pass3";
		data[2][2] = 123;
		
		return data;
	}
}
